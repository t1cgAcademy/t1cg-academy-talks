{
  "name": "gatsby-remark-copy-images",
  "description": "Find local images which are referenced from markdown and copy them to the public directory",
  "version": "0.2.1",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>, Seth Fitzsimmons <seth@mojodna.net>",
  "dependencies": {
    "fs-extra": "^1.0.0",
    "is-relative-url": "^2.0.0",
    "lodash": "^4.17.4",
    "unist-util-visit": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "cross-env": "^5.0.5"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "remark"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir .",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir ."
